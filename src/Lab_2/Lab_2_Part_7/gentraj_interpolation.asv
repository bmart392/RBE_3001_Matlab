function points = gentraj_interpolation(t0, tf, v0, vf, q0, qf, numpoints)

points = zeros(numpoints + 1, 2);
time = 0:(tf/numpoints):tf;
disp(

    for n = 1:numpoints

        coeffs = gencoeff(t0, tf, v0, vf, q0, qf);

        points(n,:) = genpoints(coeffs,time(n));  
    end
    points(numpoints+1,:) = genpoints(coeffs,tf);
     
    
end

